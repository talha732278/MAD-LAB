import 'package:flutter/material.dart';
import 'dart:async';
import 'package:flutter_animate/flutter_animate.dart';

void main() => runApp(MyApp());

class MyApp extends StatelessWidget {
  @override
  Widget build(BuildContext context) {
    return MaterialApp(
      title: 'Food App',
      debugShowCheckedModeBanner: false,
      home: SplashScreen(),
    );
  }
}

class SplashScreen extends StatefulWidget {
  @override
  _SplashScreenState createState() => _SplashScreenState();
}

class _SplashScreenState extends State<SplashScreen> {
  @override
  void initState() {
    super.initState();
    Timer(Duration(seconds: 2), () {
      Navigator.pushReplacement(
        context,
        MaterialPageRoute(builder: (_) => FoodListScreen()),
      );
    });
  }

  @override
  Widget build(BuildContext context) {
    return Scaffold(
      body: Center(
        child: Image.network(
          'data:image/jpeg;base64,',
        ),
      ),
    );
  }
}

class FoodListScreen extends StatelessWidget {
  final List<FoodItem> foodItems = [
    FoodItem(
      image:
          'data:image/jpeg;base64,',
      name: 'Pizza',
      description: 'Delicious cheesy pizza',
    ),
    FoodItem(
      image:
          'data:image/jpeg;base64,',
      name: 'Burger',
      description: 'Juicy beef burger',
    ),
  ];

  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(title: Text('Food List')),
      body: ListView.builder(
        itemCount: foodItems.length,
        itemBuilder: (context, index) {
          return FoodItemWidget(item: foodItems[index]);
        },
      ),
    );
  }
}

class FoodItem {
  final String image, name, description;

  FoodItem({
    required this.image,
    required this.name,
    required this.description,
  });
}

class FoodItemWidget extends StatefulWidget {
  final FoodItem item;

  const FoodItemWidget({Key? key, required this.item}) : super(key: key);

  @override
  State<FoodItemWidget> createState() => _FoodItemWidgetState();
}

class _FoodItemWidgetState extends State<FoodItemWidget> {
  bool isLiked = false;
  bool playAnimation = false;

  @override
  Widget build(BuildContext context) {
    return Card(
      margin: EdgeInsets.all(8),
      child: ListTile(
        contentPadding: EdgeInsets.all(8),
        leading: Image.network(widget.item.image, width: 60, height: 60),
        title: Text(widget.item.name),
        subtitle: Text(widget.item.description),
        trailing: GestureDetector(
          onDoubleTap: () {
            setState(() {
              isLiked = !isLiked;
              playAnimation = true;
            });
          },
          child: Animate(
            effects:
                playAnimation
                    ? [
                      ScaleEffect(duration: 300.ms),
                      ShakeEffect(duration: 300.ms),
                    ]
                    : [],
            onComplete: (_) => setState(() => playAnimation = false),
            child: Icon(
              Icons.favorite,
              color: isLiked ? Colors.red : Colors.grey,
              size: 28,
            ),
          ),
        ),
      ),
    );
  }
}

